<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
	tools:context=".MainActivity" >



    <Button
        android:id="@+id/newEntryButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
		android:onClick="onNewEntryButtonClicked"
        android:text="@string/button_newEntry"
		android:layout_alignParentTop="true"
		android:layout_alignParentLeft="true"
		android:layout_toLeftOf="@+id/takePhotoButton"/>

    <Button
        android:id="@+id/takePhotoButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
		android:onClick="onTakePhotoClicked"
        android:text="@string/button_takePhoto"
		android:layout_alignParentTop="true"
		android:layout_toLeftOf="@+id/newMoodButton"/>

    <Button
        android:id="@+id/newMoodButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
		android:onClick="moods"
        android:text="@string/button_newMood"
		android:layout_alignParentTop="true"
		android:layout_alignParentRight="true"/>

	<ListView
		android:id="@+id/listView1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/newMoodButton">

	</ListView>

</RelativeLayout>